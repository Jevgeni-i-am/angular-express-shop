{
  "name": "angular-shop",
  "version": "1.0.0",
  "description": "Fullstack app with Angular and Node-express",
  "main": "index.js",
  "engines": {
    "node": "14.4.0",
    "npm": "6.14.9"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client-install": "npm i --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
  },
  "keywords": [
    "Angular",
    "Node.js express"
  ],
  "author": "Jevgeni",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jevgeni-i-am/angular-express-shop.git"
  },
  "bugs": {
    "url": "https://github.com/Jevgeni-i-am/angular-express-shop/issues"
  },
  "homepage": "https://github.com/Jevgeni-i-am/angular-express-shop",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.22.1",
    "mongoose": "^5.11.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node.js": "0.0.1-security",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.1",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
